<li class="nav-item px-3">
    <NavLink class="nav-link" href="@URL" Match="@LinkMatch">
        <RadzenIcon Icon="@Icon" class="pe-2" /> @Label
    </NavLink>
</li>



@code {
    [Parameter]
    public string? URL { get; set; }

    [Parameter]
    public string? Label { get; set; }

    [Parameter]
    public string? Icon { get; set; }

    [Parameter]
    public NavLinkMatch LinkMatch { get; set; } = NavLinkMatch.Prefix;
}